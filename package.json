{
  "name": "pokemon-tcg-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "now": "next now",
    "deploy": "serverless deploy",
    "lint:src": "eslint --fix './src/**/*.{ts,tsx}'",
    "lint:pages": "eslint --fix './pages/**/*.{ts,tsx}'",
    "lint": "yarn run lint:src && yarn run lint:pages",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@auth0/nextjs-auth0": "^0.5.0",
    "@next/bundle-analyzer": "^9.1.6",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-server-micro": "^2.9.15",
    "axios": "^0.19.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "dotenv": "^8.2.0",
    "graphql-tag": "^2.10.1",
    "graphql-toolkit": "^0.7.5",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.1.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "swr": "^0.1.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add"
    ],
    "*.tsx": [
      "yarn lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/next": "^9.0.0",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.15",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "serverless-next.js": "^1.7.0",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.3"
  }
}
